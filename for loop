package vansh;
import java.util.*;
public class forloop {

  public static void main(String args[]) 
    { 
       int i,j,n;
       System.out.println("Enter the number");
       Scanner sc = new Scanner(System.in);
       n=sc.nextInt();
       for(i=0;i<n;i++){
           for(j=0;j<=i;j++){
               System.out.print(j+1);
           }
       System.out.println();
       }
       
    }
}

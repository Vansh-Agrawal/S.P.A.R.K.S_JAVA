package vansh;
import java.util.*;
public class multiplication {
	public static void table(int x, int y){
	    if(y != 1){
	      table(x,y-1);
	    }
	    System.out.println(x+"*"+y+"= "+ x*y);
	  }
	public static void main(String[] args) {
		  Scanner sc=new Scanner (System.in);
		  System.out.println("Enter a Number:");
		  int n=sc.nextInt();
	      table(n, 10);
	  }
}


